{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "extends": ["airbnb"],
  "plugins": ["prettier", "import", "react-hooks"],
  "ignorePatterns": ["node_modules/"],
  "rules": {
      "prettier/prettier": "error"
  }
}